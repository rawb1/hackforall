{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/essential", "@vue/prettier", "koa"],
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
